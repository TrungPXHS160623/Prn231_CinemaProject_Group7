<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <title>Cinema</title>
    <!-- Bootstrap -->
    <link href="~/lib/bootstrap/css/bootstrap.css" rel="stylesheet" />
    <!-- Animate.css -->
    <link href="~/lib/animate.css/animate.css" rel="stylesheet" />
    <!-- Font Awesome iconic font -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css" rel="stylesheet" />
    <!-- Magnific Popup -->
    <link href="~/lib/magnific-popup/magnific-popup.css" rel="stylesheet" />
    <!-- Slick carousel -->
    <link href="~/lib/slick/slick.css" rel="stylesheet" />
    <!-- Data table -->
    <link rel="stylesheet" href="https://cdn.datatables.net/1.11.5/css/jquery.dataTables.min.css">
    <!-- Fonts -->
    <link href='https://fonts.googleapis.com/css?family=Oswald:300,400,500,700' rel='stylesheet' type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700' rel='stylesheet' type='text/css'>
    <!-- Theme styles -->
    <link href="~/css/dot-icons.css" rel="stylesheet" />
    <link href="~/css/theme.css" rel="stylesheet" />
</head>
<body class="body">
    <header class="header header-horizontal header-view-pannel">
        <div class="container">
            <nav class="navbar">
                <a class="footer-logo" asp-page="/Admin/Index">
                    <span class="logo-element">
                        <span class="logo-tape">
                            <span class="svg-content svg-fill-theme" data-svg="/lib/images/svg/logo-part.svg"></span>
                        </span>
                        <span class="logo-text text-uppercase">
                            <span>C</span>inema
                        </span>
                    </span>
                </a>
                <button class="navbar-toggler" type="button">
                    <span class="th-dots-active-close th-dots th-bars">
                        <span></span>
                        <span></span>
                        <span></span>
                    </span>
                </button>
                <div class="navbar-collapse">
                    <ul class="navbar-nav">
                        <li class="nav-item ">
                            <a class="nav-link" asp-page="/Staff/Orders/List">Order List</a>
                        </li>
                        <li class="nav-item ">
                            <a class="nav-link" asp-page="/Staff/Orders/Add">Order Add</a>
                        </li>
                    </ul>
                    <div class="navbar-extra">
                        <ul class="navbar-nav">
                            <li class="nav-item nav-item-arrow-down nav-hover-show-sub">
                                <a class="nav-link" asp-page="/Admin/Concessions/List">User</a>
                                <div class="nav-arrow"><i class="fas fa-chevron-down"></i></div>
                                <ul class="collapse nav">
                                    <li class="nav-item">
                                        <a class="nav-link" asp-page="#">Log out</a>
                                    </li>
                                </ul>
                            </li>
                        </ul>
                    </div>
                </div>
            </nav>
        </div>
    </header>
    <main role="main" class="pb-3">
        @RenderBody()
    </main>
    <a class="scroll-top disabled" asp-page="#"><i class="fas fa-angle-up" aria-hidden="true"></i></a>
    <footer class="section-text-white footer footer-links bg-darken">
        <div class="footer-body container">
            <div class="row">
                <div class="col-sm-6 col-lg-3">
                    <a class="footer-logo" href="./">
                        <span class="logo-element">
                            <span class="logo-tape">
                                <span class="svg-content svg-fill-theme" data-svg="/lib/images/svg/logo-part.svg"></span>
                            </span>
                            <span class="logo-text text-uppercase">
                                <span>C</span>inema
                            </span>
                        </span>
                    </a>
                </div>
                <div class="col-sm-6 col-lg-3">
                    <ul class="list-unstyled list-wide footer-content">
                        <li>
                            <a class="content-link" asp-page="/Staff/Orders/List">Order List</a>
                        </li>
                        <li>
                            <a class="content-link" asp-page="/Staff/Orders/Add">Order Add</a>
                        </li>
                    </ul>
                </div>
                <div class="col-sm-6 col-lg-3">
                    <ul class="list-unstyled list-wide footer-content">
                        
                    </ul>
                </div>
                <div class="col-sm-6 col-lg-3">
                    <ul class="list-unstyled list-wide footer-content">
                        
                    </ul>
                </div>
            </div>
        </div>
    </footer>
    <!-- jQuery library -->
    <script src="~/js/jquery-3.3.1.js"></script>
    <!-- Bootstrap -->
    <script src="~/lib/bootstrap/js/bootstrap.js"></script>
    <!-- Paralax.js -->
    <script src="~/lib/parallax.js/parallax.js"></script>
    <!-- Waypoints -->
    <script src="~/lib/waypoints/jquery.waypoints.min.js"></script>
    <!-- Slick carousel -->
    <script src="~/lib/slick/slick.min.js"></script>
    <!-- Magnific Popup -->
    <script src="~/lib/magnific-popup/jquery.magnific-popup.min.js"></script>
    <!-- Data table -->
    <script src="https://cdn.datatables.net/1.11.5/js/jquery.dataTables.min.js"></script>
    <!-- Chart.js -->
    <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
    <!-- Inits product scripts -->
    <script src="~/js/script.js"></script>
    <script async defer src="https://ia.media-imdb.com/images/G/01/imdb/plugins/rating/js/rating.js"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>